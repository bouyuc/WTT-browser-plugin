{
  "manifest_version": 3,
  "version": "1.0",
  "name": "WTT Extension",
  "description": "An unofficial plugin to make the WTT website friendlier for users",
  "content_scripts": [
    {
      "matches": ["*://worldtabletennis.com/*"],
      "js": [
        "locationList.js",
        "library/moment.js",
        "library/moment-timezone-with-data.js",
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
